<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.wso2.esb</groupId>
  <artifactId>TestOrderBeanMediator</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  
    <dependencies>
        <dependency>
            <groupId>org.apache.synapse</groupId>
            <artifactId>synapse-core</artifactId>
            <version>2.1.0-wso2v2</version>
        </dependency>
        <dependency>
            <groupId>org.wso2.carbon</groupId>
            <artifactId>mediation-initializer</artifactId>
            <version>3.2.0</version>
        </dependency>
         <dependency>
            <groupId>org.apache.axis2</groupId>
            <artifactId>axis2-kernel</artifactId>
            <version>${axis2.version}</version>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <version>${commons.logging.version}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.apache.ws.commons.axiom</groupId>
            <artifactId>axiom-api</artifactId>
            <version>${axiom.version}</version>
            <scope>provided</scope>
        </dependency>
     	<dependency>
	  		<groupId>org.wso2</groupId>
	         <artifactId>example.model</artifactId>
	         <scope>system</scope>
	         <version>0.0.1-SNAPSHOT</version>
	         <systemPath>${basedir}/src/main/resources/lib/example.model-0.0.1-SNAPSHOT.jar</systemPath>
 		</dependency>
 	 	<dependency>
            <groupId>org.milyn.wso2</groupId>
            <artifactId>smooks</artifactId>
            <version>1.4.0.wso2v1</version>            
        </dependency>
   </dependencies>
   
    <build>
      <plugins>
        <plugin>
          <groupId>org.apache.maven.plugins</groupId>
          <artifactId>maven-compiler-plugin</artifactId>
          <version>2.0</version>
          <configuration>
            <source>1.5</source>
            <target>1.5</target>
          </configuration>
        </plugin>
      </plugins>
    </build>
   
    <properties>
        <axis2.version>1.3</axis2.version>
        <axiom.version>1.2.5</axiom.version>
        <commons.logging.version>1.1</commons.logging.version>
        <geronimo.jms.spec.version>1.0</geronimo.jms.spec.version>
    </properties>
</project>
