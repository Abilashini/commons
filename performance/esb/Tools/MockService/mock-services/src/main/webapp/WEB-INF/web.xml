<web-app>
    <display-name>WSO2 Performance - Mock Services</display-name>

    <servlet>
        <servlet-name>EchoService</servlet-name>
        <servlet-class>org.wso2.performance.services.EchoService</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EchoService</servlet-name>
        <url-pattern>/EchoService</url-pattern>
    </servlet-mapping>
</web-app>
